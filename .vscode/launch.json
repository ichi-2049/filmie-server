{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch GraphQL Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/app/server.go",
            "envFile": "${workspaceFolder}/.env.debug",
            "console": "integratedTerminal",
        },
        {
            "name": "Launch Get TMDB Movie Data",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/app/cmd/local/tmdb/main.go",
            "envFile": "${workspaceFolder}/.env.debug",
            "console": "integratedTerminal",
        }
    ]
}
